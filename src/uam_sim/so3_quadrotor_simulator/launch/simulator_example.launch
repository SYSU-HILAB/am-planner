<launch>  
  <arg name="start_x" default="-14.0" />
  <arg name="start_y" default="4.0" />
  <arg name="start_z" default="1.4" />
  <arg name="scale" default="4" />

  <param name="scale" type="double" value="$(arg scale)" />
  <param name="staticR" type="double" value="0.067" />
  <param name="dynar" type="double" value="0.024" />
  <param name="upper_arm" type="double" value="0.100" />
  <param name="lower_arm" type="double" value="0.160" />
  <param name="rate" type="double" value="100.0" />
  <param name="mass" type="double" value="0.98" />
  <param name="debug" type="bool" value="false" />
  <param name="debug_endEffector" value="false" />

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find so3_quadrotor_simulator)/rviz/visual.rviz" output="screen" />

    <!-- simulator -->
    <include file="$(find so3_quadrotor_simulator)/launch/simulator.xml">
        <arg name="start_x" value="$(arg start_x)"/>
        <arg name="start_y" value="$(arg start_y)"/>
        <arg name="start_z" value="$(arg start_z)"/>
        <arg name="scale" value="$(arg scale)"/>
    </include>
 </launch>



